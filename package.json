{
  "name": "webgazer",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/brownhci/WebGazer.git"
  },
  "license": "LGPL-3.0-or-later",
  "main": "dist2/webgazer.commonjs2.js",
  "module": "src/index.mjs",
  "scripts": {
    "clean": "rimraf dist2",
    "prebuild": "npm run clean",
