    "husky": "^4.2.5",
    "localforage": "1.7.3",
    "numeric": "1.2.6",
    "regression": "2.0.1"
  },
  "devDependencies": {
    "file-loader": "^6.0.0",
    "parallel-webpack": "2.6.0",
    "rimraf": "2.6.3",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  }
}
